<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.webdroidteam.teste_banco_2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.webdroidteam.teste_banco_2.MainActivity"
            android:label="Todo List" />
        <activity
            android:name="com.webdroidteam.teste_banco_2.LoginActivity"
            android:label="Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.webdroidteam.teste_banco_2.CadUsuario"
            android:label="Cadastro de Usuário" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".ListUsuariosActivity" />

        <activity
            android:name="com.webdroidteam.teste_banco_2.ListUsuariosActivity"
            android:label="Usuários" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>